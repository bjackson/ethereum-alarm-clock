{
    "extends": "airbnb-base",
    "env": {
        "node": true,
        "mocha": true
    },
    "globals": {
        "artifacts": "readonly",
        "web3": "readonly",
        "contract": "readonly"
    },
    "rules": {
        "semi": "warn",
        "quotes": "off",
        "no-unused-expressions": "off",
        "comma-dangle": "off",
        "max-len": ["warn", {
            "code": 120
        }]
    }
}
